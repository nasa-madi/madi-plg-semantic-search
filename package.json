  "types": "lib/",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
